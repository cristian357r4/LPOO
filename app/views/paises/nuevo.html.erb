<h1>Formulario Pais</h1>

<%= form_with(model: @pais,url: pais_crear_path ,local: true) do |form| %> 

  <div class="field">
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div class="field">
    <%= form.label :descripcion %>
    <%= form.text_field :descripcion %>
  </div>

  <div class="actions">
    <%= form.submit "Guardar"%>
  </div>


<% end %>

<%= link_to 'Regresar', paises_inicio_path %>